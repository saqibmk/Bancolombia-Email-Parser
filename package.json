{
  "name": "miFinance",
  "version": "1.0.0",
  "description": "A personal finance app",
  "main": "index.js",
  "author": "Saqib Khan <saqibmk@users.noreply.github.com>",
  "license": "MIT",
  "scripts": {
    "start": "nodemon src/app.js --ignore db.json --exec babel-node --presets babel-preset-env",
    "init": "babel-node src/init.js --presets babel-preset-env",
    "client": "cd src/client && yarn start",
    "server": "nodemon src/server.js --exec babel-node --presets babel-preset-env",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "concurrently": "^3.5.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.10.0",
    "nodemon": "^1.17.3"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.3",
    "google-auth-library": "^1.3.2",
    "googleapis": "^28.0.1",
    "inquirer": "^5.2.0",
    "lowdb": "^1.0.0",
    "luxon": "^1.0.0"
  }
}
